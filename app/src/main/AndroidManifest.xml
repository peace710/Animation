<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.peace.animation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Animation">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.scene.ActivitySceneTransition"/>
        <activity android:name=".activity.scene.DetailActivity"/>
        <activity android:name=".basic.transition.BasicTransitionActivity"/>
        <activity android:name=".custom.transition.CustomTransitionActivity"/>
        <activity android:name=".drawable.animation.DrawableAnimationActivity"/>
    </application>

</manifest>